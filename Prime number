#Prime number

def primeno(n):
    count=0
    if(n>1):
        
        for i in range(1,n+1):
                if(n%i==0):
                    count=count+1
        if(count==2):
            return True
        else:
            return False
   
    
x= int(input("Enter a value"))
print(primeno(x))
