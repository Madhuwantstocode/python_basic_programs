#sum of elements in array
list1=[5,6,7,8,4]

print(sum(list1))
#print(max(list1), min(list1))

max1=list1[0]
min1=list1[0]
for i in range(0,len(list1)):
    if(list1[i]>max1):
        max1=list1[i]
    if(list1[i]<min1):
        min1=list1[i]
print("Maximum number is", max1)
print("Minimum number is", min1)
